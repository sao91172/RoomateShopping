<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.RoomateShopping"
        tools:targetApi="31">
        <activity
            android:name=".PurchasedRecyclerAdapter"
            android:exported="false" />
        <activity
            android:name=".TotalCostPageActivity"
            android:parentActivityName=".SettleCostActivity"
            android:exported="false" />
        <activity
            android:name=".SettleCostActivity"
            android:parentActivityName=".Navigation"
            android:exported="false" />
        <activity
            android:name=".CartActivity"
            android:parentActivityName=".Navigation"
            android:exported="false" />
        <activity
            android:name=".ItemRecyclerAdapter"
            android:exported="false" />
        <activity
            android:name=".PersonalShoppingList"
            android:parentActivityName=".Navigation"
            android:exported="false" />
        <activity
            android:name=".AddItemActivity"
            android:parentActivityName=".Navigation"
            android:exported="false" />
        <activity
            android:name=".Navigation"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".RegisterActivity"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>